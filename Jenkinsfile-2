pipeline {
    agent {
        label 'Agent-1'
    }
    options {
        ansiColor('xterm')
    }
    stages {
        stage('sample') {
            environment {
                SSH_CREDS = credentials('my-predefined-ssh-creds')
            }
            steps{
               sh '''
                echo "pesi"
                printenv
               ''' 
            }
        }
    }
}